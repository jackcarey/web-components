{
  "author": "jackcarey (jackcarey.co.uk)",
  "dependencies": {
    "@npmcli/package-json": "^6.1.1",
    "@types/ical": "^0.8.3",
    "ical.js": "^2.1.0"
  },
  "description": "A collection of web components for a variety purposes.",
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@custom-elements-manifest/analyzer": "^0.10.4",
    "@jest/globals": "^29.7.0",
    "@npmcli/package-json": "^6.1.1",
    "@playwright/test": "^1.49.1",
    "@storybook/addon-a11y": "^8.4.7",
    "@storybook/addon-coverage": "^1.0.5",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-themes": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/builder-vite": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@storybook/test-runner": "^0.21.0",
    "@storybook/web-components": "^8.4.7",
    "@storybook/web-components-vite": "^8.4.7",
    "axe-playwright": "^2.0.3",
    "chromatic": "^11.25.2",
    "concurrently": "^9.1.2",
    "custom-elements-manifest": "^2.1.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "lit": "^3.2.0",
    "storybook": "^8.5.3",
    "wait-on": "^8.0.2"
  },
  "keywords": [],
  "license": "ISC",
  "main": "autoloader.js",
  "name": "web-components",
  "scripts": {
    "build-storybook": "node scripts/generate-storybook-docs.mjs && storybook build",
    "storybook": "storybook dev -p 6006",
    "test": "test-storybook --ci --json --failOnConsole --includeTags=\"test,a11y-minor,a11y-major,a11y-serious,a11y-critical\" --excludeTags=\"skip\"",
    "test-coverage": "test --coverage",
    "test-ci": "npm run build-storybook && concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"npx http-server storybook-static --silent --port 6006\" \"wait-on tcp:6006 && npm run test\"",
    "build-docs": "npm i && node scripts/install-deps-at-root.mjs && node scripts/generate-storybook-docs.mjs && node scripts/generate-pkg-readmes.mjs && node scripts/generate-root-readme.mjs && storybook build",
    "serve-ci": "npm run build-docs && npm run storybook",
    "chromatic": "npx chromatic"
  },
  "version": "1.0.0",
  "packageManager": "pnpm@8.15.5+sha1.a58c038faac410c947dbdb93eb30994037d0fce2"
}
