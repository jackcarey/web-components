{
  "author": "jackcarey (jackcarey.co.uk)",
  "dependencies": {
    "@npmcli/package-json": "^5.2.0",
    "@types/ical": "^0.8.3",
    "ical.js": "^2.1.0"
  },
  "description": "A collection of web components for a variety purposes.",
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.3",
    "@custom-elements-manifest/analyzer": "^0.10.3",
    "@jest/globals": "^29.7.0",
    "@npmcli/package-json": "^5.2.0",
    "@playwright/test": "^1.49.1",
    "@storybook/addon-a11y": "^8.3.1",
    "@storybook/addon-coverage": "^1.0.4",
    "@storybook/addon-essentials": "^8.3.1",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-themes": "^8.4.7",
    "@storybook/blocks": "^8.2.9",
    "@storybook/test": "^8.3.1",
    "@storybook/test-runner": "^0.19.1",
    "@storybook/web-components": "^8.4.7",
    "@storybook/web-components-vite": "^8.4.7",
    "axe-playwright": "^2.0.3",
    "concurrently": "^9.1.0",
    "custom-elements-manifest": "^2.1.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "lit": "^3.2.0",
    "storybook": "^8.4.7",
    "wait-on": "^8.0.1"
  },
  "keywords": [],
  "license": "ISC",
  "main": "autoloader.js",
  "name": "web-components",
  "scripts": {
    "storybook:build": "storybook build && touch ./storybook-static/.nojekyll",
    "storybook": "storybook dev -p 6006",
    "test": "test-storybook --ci --json --failOnConsole --includeTags=\"test,a11y-minor,a11y-major,a11y-serious,a11y-critical\" --excludeTags=\"skip\"",
    "test:coverage": "test --coverage",
    "test:ci": "npm run storybook:build && concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"npx http-server storybook-static --port 6006\" \"wait-on tcp:6006 && npm run test\"",
    "build:docs": "npm i && node scripts/install-deps-at-root.mjs && node scripts/generate-storybook-docs.mjs && node scripts/generate-pkg-readmes.mjs && node scripts/generate-root-readme.mjs && storybook build",
    "serve:ci": "npm run build:docs && npm run storybook"
  },
  "version": "1.0.0",
  "packageManager": "pnpm@8.15.5+sha1.a58c038faac410c947dbdb93eb30994037d0fce2"
}