{
  "author": "jackcarey (jackcarey.co.uk)",
  "dependencies": {
    "@npmcli/package-json": "^5.2.0",
    "ical.js": "^2.0.1"
  },
  "description": "A collection of web components for a variety purposes.",
  "devDependencies": {
    "@chromatic-com/storybook": "^1.7.0",
    "@custom-elements-manifest/analyzer": "^0.10.3",
    "@jest/globals": "^29.7.0",
    "@npmcli/package-json": "^5.2.0",
    "@storybook/addon-a11y": "^8.2.9",
    "@storybook/addon-coverage": "^1.0.4",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-themes": "^8.2.9",
    "@storybook/blocks": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@storybook/test-runner": "^0.19.1",
    "@storybook/web-components": "^8.2.9",
    "@storybook/web-components-vite": "^8.2.9",
    "concurrently": "^8.2.2",
    "custom-elements-manifest": "^2.1.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "lit": "^3.2.0",
    "storybook": "^8.2.9",
    "wait-on": "^7.2.0"
  },
  "keywords": [],
  "license": "ISC",
  "main": "autoloader.js",
  "name": "web-components",
  "scripts": {
    "build-storybook": "storybook build --quiet",
    "storybook": "storybook dev -p 6006",
    "test": "test-storybook --verbose --json --failOnConsole",
    "test:coverage": "test --coverage",
    "test:ci": "npm run build-storybook --quiet && concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"npx http-server storybook-static --port 6006 --silent\" \"wait-on tcp:6006 && npm run test:coverage --quiet\""
  },
  "version": "1.0.0"
}